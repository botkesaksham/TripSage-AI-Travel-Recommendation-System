{
    "version": 2,
    "builds": [
      {
        "src": "api.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api.py"
      }
    ],

    "functions": {
      "api.py": {
        "memory": 1024, 
        "maxDuration": 10 
      }
    }
  }
  