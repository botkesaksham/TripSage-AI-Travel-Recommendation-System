{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/plan_trip",
      "methods": ["POST"],
      "dest": "api.py"
    },
    {
      "src": "/(.*)",
      "dest": "api.py"
    }
  ],
  "functions": {
    "api/*": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}

