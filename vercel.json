{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.11" }
    }
  ],
  "routes": [
    {
      "src": "/plan_trip",
      "methods": ["POST"],
      "dest": "api.py"
    },
    {
      "src": "/(.*)",
      "dest": "api.py"
    }
  ]
}
